{
  "name": "add-dot",
  "displayName": "傍点",
  "publisher": "kaorin",
  "description": "指定した範囲に傍点を追加する",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/AddDot.png",
  "activationEvents": [
    "onCommand:extension.addDot",
    "onCommand:extension.addRuby"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addDot",
        "title": "傍点"
      },
      {
        "command": "extension.addRuby",
        "title": "ルビ"
      }
    ],
    "keybindings": [
      {
        "command": "extension.addDot",
        "key": "ctrl+shift+/",
        "mac": "cmd+shift+/"
      },
      {
        "command": "extension.addRuby",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      }
    ]
  },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/kaorin/AddDot/issues",
    "email": "kaoru.konno@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaorin/AddDot.git"
  },
  "homepage": "https://github.com/kaorin/AddDot/blob/master/README.md"
}
